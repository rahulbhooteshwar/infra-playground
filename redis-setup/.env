# Redis Configuration
REDIS_PORT=6379

# Authentication Settings
# Set REDIS_AUTH_ENABLED to "true" to enable authentication, "false" to disable
REDIS_AUTH_ENABLED=false

# Redis password (only used when REDIS_AUTH_ENABLED=true)
# Uncomment and set a strong password when enabling authentication
# REDIS_PASSWORD=your_secure_password_here

# Example with authentication enabled:
# REDIS_AUTH_ENABLED=true
# REDIS_PASSWORD=MySecureRedisPassword123!